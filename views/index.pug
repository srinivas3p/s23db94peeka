extends layout

block content
  h1= title
  p I am Srinivas, front-end developer
  button(type="button", class="btn btn-secondary", onclick="goFaster()") Press Me
  #faster Increase the speed

  script.
    var mph = 20;
    var change = 1;

    function goFaster() {
      mph += change;
      change += 1;
      document.getElementById('faster').textContent = 'MPH ' + mph + ' with change ' + change;
    }
  a(href="/nwclass") Back to Search